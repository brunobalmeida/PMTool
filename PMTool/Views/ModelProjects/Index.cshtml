@model PMTool.Models.ModelProject

@{
    ViewData["Title"] = "Load Project Model";
}

<h2 class="display text-center text-uppercase font-weight-bold text-primary">@ViewData["Title"]</h2>

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="jumbotron">
                <form asp-action="Create">
                    <label asp-for="ModelProjectId" class="control-label"></label>
                    <select asp-for="ModelProjectId" name="id" id="id" class="form-control" asp-items="ViewBag.ModelProjectId"></select>
                    <br />
                    <input type="submit" value="Load Model" class="btn btn-outline-dark" />
                </form>
            </div>
        </div>
    </div>
</div>